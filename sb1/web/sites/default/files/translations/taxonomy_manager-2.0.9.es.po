msgid "Delete"
msgstr "Eliminar"
msgid "Taxonomy"
msgstr "Taxonomía"
msgid "Search"
msgstr "Buscar"
msgid "Settings"
msgstr "Configuración"
msgid "Export"
msgstr "Exportar"
msgid "Move"
msgstr "Mover"
